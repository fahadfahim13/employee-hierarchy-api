NODE_ENV=development
PORT=3000

# Database
DB_HOST=postgres        # Must be the service name, not localhost
DB_PORT=5432           # PostgreSQL default port inside container
DB_USERNAME=postgres   # Default PostgreSQL user
DB_PASSWORD=postgres   # Your chosen password
DB_DATABASE=employee   # Your database name

# JWT
JWT_SECRET=JWT_SECRET_KEY
JWT_EXPIRES_IN=1d